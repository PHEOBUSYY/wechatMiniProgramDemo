<!--pages/create/create.wxml-->
<view>
  <form bindsubmit="formSubmit" bindreset="formReset">

  <view class="section">
    <view class="section__title">标题</view>
    <input name="title" placeholder="please input here" />
  </view>
  <view class="section">
    <view class="section__title">url</view>
    <input name="url" placeholder="please input here" />
  </view>
   <view class="section">
    <view class="section__title">logo</view>
    <input name="logo" placeholder="please input here" />
  </view>
   <view class="section">
    <view class="section__title">描述</view>
    <input name="content" placeholder="please input here" />
  </view>
  <view class="btn-area">
    <button formType="submit">Submit</button>
    <button formType="reset">Reset</button>
  </view>
  </form>
</view>